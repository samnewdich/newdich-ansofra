{
    "name": "newdich/ansofra",
    "description": "Ansofra is a software Architecture framework that follows the CQRS pattern in PHP, grouping all important products into group, each product being a microservice.",
    "type": "library",
    "license": "MIT",
    "version": "2.1.8",
    "authors": [
        {
            "name": "Sam Newdich",
            "email": "samnewdich@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "vlucas/phpdotenv": "^5.6",
        "phpmailer/phpmailer": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ansofra\\Bin\\": "bin/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ansofra\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/ansofra"
    ],
    "scripts": {
        "post-autoload-dump": [
            "@php -r \"echo 'Ansofra framework autoloaded successfully.' . PHP_EOL;\""
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
